{
  "name": "@freik/audiodb",
  "version": "0.8.3",
  "description": "NodeJS Audio/Music database",
  "author": "Kevin Frei <kevinfrei@hotmail.com>",
  "license": "Unlicense",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://kevinfrei/audio-database.git"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "watch": "jest --config jestconfig.json --watch",
    "clean": "rimraf lib && rimraf \".*.tsbuildinfo\"",
    "build": "tsc --project tsconfig.json",
    "cleanbuild": "yarn clean && yarn build",
    "dbg": "yarn clean && tsc --sourceMap true --project tsconfig.json",
    "prepare": "yarn build",
    "postpublish": "pinst --enable",
    "prepublishOnly": "yarn test && yarn lint && pinst --disable",
    "lint": "eslint --fix --ext .ts src",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags --no-verify",
    "compile": "tsc --noEmit --project tsconfig.json",
    "linecount": "git ls-files|grep \"\\.\\(css\\|ts\\)$\"|grep -v \"__\\|\\.d\\.ts\"|xargs wc -l",
    "chk": "yarn lint && yarn compile && yarn test",
    "doc": "typedoc ./src/index.ts --readme none --tsconfig tsconfig.json"
  },
  "dependencies": {
    "@freik/containers": "^0.2.1",
    "@freik/helpers": "^0.3.0",
    "@freik/logger": "^0.2.0",
    "@freik/media-core": "^0.13.0",
    "@freik/media-utils": "^0.14.3",
    "@freik/node-utils": "^1.3.3",
    "@freik/search": "^1.1.0",
    "@freik/text": "^0.3.1",
    "@freik/typechk": "^0.4.0",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@freik/build-tools": "^3.6.0",
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.2",
    "@types/rmfr": "^2.0.2",
    "@types/xxhashjs": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rmfr": "^2.0.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "5.0.4"
  },
  "files": ["lib/**/*"]
}
