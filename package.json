{
  "license": "CC0-1.0",
  "name": "emp",
  "version": "0.8.8",
  "private": true,
  "description": "Electron Music Player",
  "author": "Kevin Frei <kevinfrei@hotmail.com>",
  "main": "dist-electron/main.js",
  "homepage": "./",
  "workspaces": ["shared", "packages/*"],
  "scripts": {
    "clean": "rimraf public/main public/*.js build dist",
    "testui": "jest --config config/jest.jsdom.js --watch",
    "testnode": "jest --config config/jest.node.js --watch",
    "test": "jest --config config/jest.jsdom.js && jest --pass-with-no-tests --config config/jest.node.js",
    "format": "ftool format",
    "lint": "yarn eslint --fix --ext .ts --ext .tsx ./",
    "linecount": "ftool linecount .css .ts .tsx .html -__ -.d.ts",
    "postinstall": "husky install",
    "chk": "yarn tsc && yarn lint && yarn test",
    "verbump": "node config/set-version.js",
    "bumpver": "node config/set-version.js",
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fluentui/react": "^8.110.3",
    "@fluentui/react-components": "^9.21.0",
    "@fluentui/react-icons-mdl2": "^1.3.45",
    "@freik/audiodb": "^0.8.3",
    "@freik/containers": "^0.2.0",
    "@freik/electron-main": "^0.1.0",
    "@freik/electron-preload": "^0.1.0",
    "@freik/electron-render": "^0.1.0",
    "@freik/emp-shared": "^0.0.6",
    "@freik/helpers": "^0.3.0",
    "@freik/logger": "^0.2.0",
    "@freik/media-core": "^0.12.0",
    "@freik/media-utils": "~0.14.2",
    "@freik/node-utils": "~1.3.3",
    "@freik/p-limit": "^5.0.0",
    "@freik/sync": "^0.2.0",
    "@freik/text": "^0.3.0",
    "@freik/typechk": "^0.2.0",
    "@freik/web-utils": "^0.5.2",
    "album-art": "^3.1.0",
    "jimp": "^0.22.8",
    "open": "^9.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.7.7",
    "rimraf": "^5.0.1",
    "xxhashjs": "^0.2.2"
  },
  "browserslist": {
    "production": ["last 1 chrome version"],
    "development": ["last 1 chrome version"]
  },
  "devDependencies": {
    "@freik/build-tools": "3.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/debug": "^4.1.8",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^17.0.62",
    "@types/react-dom": "^17.0.20",
    "@types/react-test-renderer": "^17.0.2",
    "@types/xxhashjs": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.0.0",
    "cross-env": "^7.0.3",
    "electron": "^25.1.1",
    "electron-builder": "^24.4.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-css": "^6.0.1",
    "lerna": "^7.0.2",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^17.0.2",
    "svg-react-loader": "^0.4.6",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-electron": "^0.12.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "packageManager": "yarn@3.6.0"
}
