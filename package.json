{
  "license": "CC0-1.0",
  "name": "EMP",
  "version": "0.1.0",
  "private": true,
  "description": "Electron Music Player",
  "author": "Kevin Frei <kevinfrei@hotmail.com>",
  "build": {
    "appId": "<com.music-player.freik>"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "csstype": "^2.6.11",
    "electron-is-dev": "^1.2.0",
    "electron-promise-ipc": "^2.1.0",
    "electron-react-devtools": "^0.5.3",
    "media-utils": "file:./submodules/media-utils",
    "my-utils": "file:./submodules/js-freik-utils",
    "node-mediainfo": "^1.0.3",
    "react": "^16.13.1",
    "react-bootstrap": "^1.2.2",
    "react-dom": "^16.13.1",
    "react-eternal-list": "^1.0.6",
    "react-scripts": "3.4.1",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "recoil": "^0.0.10",
    "rxjs": "^6.6.0",
    "simplelogger": "file:./submodules/simplelogger",
    "source-map-explorer": "^2.4.2",
    "undux": "^5.2.0"
  },
  "scripts": {
    "analyze": "yarn react-scripts build && yarn source-map-explorer 'build/static/js/*.js'",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn prepare && yarn react-build && electron-builder --publish=always",
    "build": "yarn prepare && yarn react-build && yarn electron-build",
    "start": "yarn prepare && concurrently --kill-others \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "prepare": "flow-remove-types static/ -d public/ --all --pretty",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^9.1.1",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.1.1",
    "flow-bin": "^0.129.0",
    "flow-remove-types": "^2.129.0",
    "typescript": "^3.9.7",
    "wait-on": "^5.1.0"
  }
}
