{
  "license": "CC0-1.0",
  "name": "music",
  "version": "0.1.0",
  "private": true,
  "description": "Electron Music Player",
  "author": "Kevin Frei <kevinfrei@hotmail.com>",
  "build": {
    "appId": "<com.music-player.freik>"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "amplitudejs": "^5.0.3",
    "electron-is-dev": "^1.1.0",
    "music-metadata": "^5.2.0",
    "my-utils": "../modules/js-freik-utils",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "rxjs": "^6.5.3",
    "simplelogger": "file:../modules/simplelogger",
    "source-map-explorer": "^2.1.2",
    "undux": "^5.0.1"
  },
  "scripts": {
    "analyze": "yarn react-scripts build && yarn source-map-explorer 'build/static/js/*.js'",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn prepare && yarn react-build && electron-builder --publish=always",
    "build": "yarn prepare && yarn react-build && yarn electron-build",
    "start": "yarn prepare && concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "prepare": "flow-remove-types static/ -d public/ --all --pretty",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "babel-jest": "^24.9.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.0",
    "electron": "^7.1.6",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "flow-bin": "^0.114.0",
    "flow-remove-types": "^2.114.0",
    "jest": "^24.9.0",
    "typescript": "^3.7.3",
    "wait-on": "^3.3.0"
  }
}
