{
  "license": "CC0-1.0",
  "name": "emp",
  "version": "0.1.0",
  "private": true,
  "description": "Electron Music Player",
  "author": "Kevin Frei <kevinfrei@hotmail.com>",
  "build": {
    "appId": "<com.music-player.freik>"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@dwqs/react-virtual-list": "^1.0.0",
    "@freik/core-utils": "^0.3.4",
    "@freik/media-utils": "^0.2.2",
    "@freik/simplelogger": "^1.1.3",
    "bootstrap": "^4.5.2",
    "csstype": "^3.0.3",
    "electron-is-dev": "^1.2.0",
    "electron-promise-ipc": "^2.2.1",
    "electron-react-devtools": "^0.5.3",
    "node-mediainfo": "^1.0.3",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-eternal-list": "^1.0.6",
    "react-scripts": "3.4.3",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "recoil": "^0.0.10",
    "rxjs": "^6.6.2",
    "source-map-explorer": "^2.5.0",
    "undux": "^5.2.0"
  },
  "scripts": {
    "analyze": "yarn react-scripts build && yarn source-map-explorer 'build/static/js/*.js'",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn prepare && yarn react-build && electron-builder --publish=always",
    "build": "yarn prepare && yarn react-build && yarn electron-build",
    "start": "yarn prepare && concurrently --kill-others \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "prepare": "rimraf public/main public/*.js public/*.js.map && tsc -p tsconfig.static.json && tsc -p tsconfig.render.json",
    "tsc": "tsc",
    "test": "react-scripts test",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"static/**/*.ts\"",
    "lint-src": "tslint -p tsconfig.json",
    "lint-static": "tslint -p tsconfig.static.json",
    "lint-render": "tslint -p tsconfig.render.json",
    "lint": "yarn lint-src && yarn lint-static && yarn lint-render"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@types/jest": "^26.0.12",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/recoil": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.0",
    "electron-builder": "^22.8.0",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.8.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.2.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2",
    "wait-on": "^5.2.0"
  }
}
