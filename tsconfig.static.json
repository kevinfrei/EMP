{
  "compilerOptions": {
    "target": "es5",
    "outDir": "public",
    "module": "commonjs",
    "sourceMap": true,
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
//    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "static"
  ]
}
