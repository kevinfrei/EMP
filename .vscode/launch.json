{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jest testui",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceFolder}/node_modules/.bin/jest",
        "--runInBand",
        "--config",
        "${workspaceFolder}/jest.jsdom.config.js"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Electron: Main",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "program": "${workspaceFolder}/public/electron.js",
      "runtimeArgs": [
        ".",
        "--enable-logging",
        "--remote-debugging-port=9223"
      ],
      "outputCapture": "std",
      "preLaunchTask": "npm: prepare"
    },
    {
      "name": "Electron: Render",
      "type": "chrome",
      "request": "attach",
      "port": 9223,
      "webRoot": "${workspaceFolder}/public/",
      "timeout": 30000,
      "preLaunchTask": "npm: react-start"
    },
    {
      "name": "NPM stuff",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "${workspaceRoot}/node_modules/react-scripts/bin/react-scripts.js",
        "start"
      ]
    }
  ],
  "compounds": [
    {
      "name": "Electron: All",
      "configurations": [
        "Electron: Main",
        "Electron: Render"
      ]
    }
  ]
}