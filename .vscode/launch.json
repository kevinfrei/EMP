{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jest testui",
      "windows": {
        "runtimeArgs": [
          "--inspect-brk",
          "${workspaceFolder}/node_modules/jest-cli/bin/jest.js",
          "--runInBand",
          "--config",
          "${workspaceFolder}/config/jest.jsdom.js"
        ]
      },
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceFolder}/node_modules/.bin/jest",
        "--runInBand",
        "--config",
        "${workspaceFolder}/config/jest.jsdom.js"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "port": 9229
    },
    {
      "name": "Electron: Main",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "runtimeArgs": [".", "--enable-logging", "--remote-debugging-port=9223"],
      "protocol": "inspector",
      "outputCapture": "std"
    },
    {
      "name": "Electron: Render",
      "type": "chrome",
      "request": "attach",
      "port": 9223,
      "webRoot": "${workspaceFolder}/public/",
      "timeout": 30000,
      "runtimeArgs": [".", "--enable-logging", "--remote-debugging-port=9223"],
      "preLaunchTask": "npm: react-start"
    },
    {
      "name": "Electron: Just Main?",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "sourceMaps": true,
      "outFiles": ["${workspaceRoot}/public/**/*.js"],
      "args": ["."],
      "outputCapture": "std",
      "preLaunchTask": "npm: prepare"
    },
    {
      "name": "NPM stuff",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "${workspaceRoot}/node_modules/react-scripts/bin/react-scripts.js",
        "start"
      ]
    }
  ],
  "compounds": [
    {
      "name": "Electron: All",
      "configurations": ["Electron: Main", "Electron: Render"]
    }
  ]
}
