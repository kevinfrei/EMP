{
  "search.exclude": {
    "public/**/*.js": true,
    "public/**/*.js.map": true,
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "build": true,
    "dist": true,
    "yarn*.l*": true,
    "LICENSE.md": true,
    "*.tsbuildinfo": true,
    ".gitmodules": true,
    ".eslintcache": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.formatOnSave": true,
  "prettier.configPath": ".prettierrc"
}